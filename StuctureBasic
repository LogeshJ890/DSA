#include <stdio.h>

struct Student{
    int roll;
    char name[50];
    float marks;
};

int main() {
    struct Student s1;
    
    printf("Enter Roll No :");
    scanf("%d",&s1.roll);
    printf("Enter Name: ");
    scanf("%s",&s1.name);
    printf("Enter Marks :");
    scanf("%f",&s1.marks);
    
    printf("\nStudents Details:\n");
    printf("Roll: %d\nName: %s\nMarks: %.2f\n", s1.roll ,s1.name,s1.marks);
    
    
    return 0;
}


/* ----OUTPUT----
Enter Roll No :78
Enter Name: logesh
Enter Marks :89

Students Details:
Roll: 78
Name: logesh
Marks: 89.00
*/
